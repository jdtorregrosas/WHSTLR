{
  "name": "WHSTLR",
  "version": "1.0.0",
  "description": "Print the release notes from commit messages in a repository",
  "scripts": {
    "start": "node ./app.js",
    "test": "node node_modules/.bin/mocha test/*",
    "lint": "node node_modules/eslint/bin/eslint.js",
    "lint-app": "npm run lint app/* && npm run lint app.js",
    "lint-routes": "npm run lint routes/*",
    "lint-tests": "npm run lint test/*",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- tests/* -R spec",
    "watch": "nodemon ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.local.coliquio.de:jdtorregrosas/releaseNotesPrinter.git"
  },
  "keywords": [
    "release"
  ],
  "author": "Julian",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bower": "^1.7.9",
    "eslint": "^3.0.0",
    "express": "^4.14.0",
    "fs-extra": "^0.30.0",
    "jade": "^1.11.0",
    "joi": "^9.0.0",
    "promisify-node": "^0.4.0",
    "superagent": "^2.0.0",
    "supertest": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2"
  }
}
