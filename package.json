{
  "name": "WHSTLR",
  "version": "1.0.0",
  "description": "Print the release notes from commit messages in a repository",
  "scripts": {
    "start": "node ./app.js",
    "mocha": "node_modules/.bin/mocha",
    "test": "node_modules/.bin/mocha test/*",
    "linter": "eslint app/ test/ routes/",
    "coverage": "istanbul cover _mocha",
    "checkCoverage": "istanbul check-coverage --functions 100 --branches 0 --lines 40 --statements 40",
    "watch": "nodemon ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.local.coliquio.de:jdtorregrosas/releaseNotesPrinter.git"
  },
  "keywords": [
    "release"
  ],
  "author": "Julian",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bower": "^1.7.9",
    "eslint": "^3.0.0",
    "express": "^4.14.0",
    "express3-handlebars": "^0.5.2",
    "fs-extra": "^0.30.0",
    "joi": "^9.0.0",
    "promisify-node": "^0.4.0",
    "superagent": "^2.0.0",
    "supertest": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2"
  }
}
